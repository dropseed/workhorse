    "https://github.com/flinthillsdesign/MCUSA/pull/451",
    "https://github.com/flinthillsdesign/TrinityHeightsUMC/pull/146",
    "https://github.com/flinthillsdesign/Atomic/pull/351",
    "https://github.com/flinthillsdesign/JHBanning/pull/214",
    "https://github.com/flinthillsdesign/PrairieBuildingSystems/pull/159",
    "https://github.com/flinthillsdesign/CarriageFactoryGallery/pull/291",
    "https://github.com/flinthillsdesign/upland/pull/190",
    "https://github.com/flinthillsdesign/pes/pull/17",
    "https://github.com/flinthillsdesign/prairy/pull/43"
  ],
  "config": {
    "issues": "is:open is:pr label:automerge org:flinthillsdesign",
    "steps": [
      {
        "run": "remove_label",
        "args": [
          "automerge"
        ]
      },
      {
        "run": "sleep",
